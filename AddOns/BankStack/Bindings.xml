<Bindings>
	<Binding name="BANKSTACK" description="Fill incomplete stacks in your bank from your bags" header="BANKSTACK_HEAD">
		SlashCmdList["BANKSTACK"]("bags bank")
	</Binding>
	<Binding name="COMPRESS" description="Merge stacks in your bags">
		SlashCmdList["COMPRESSBAGS"]("bags")
	</Binding>
	<Binding name="BAGSORT" description="Sort your bags">
		SlashCmdList["SORT"]("bags")
	</Binding>
</Bindings>
